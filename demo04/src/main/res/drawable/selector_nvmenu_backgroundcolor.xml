<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">

        <shape android:shape="rectangle">
            <solid android:color="#990f10f0" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:color="@android:color/holo_blue_dark" android:width="1dp" android:dashGap="10dp" android:dashWidth="10dp" />
        </shape>
    </item>
</selector>